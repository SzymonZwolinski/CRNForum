<MudDialog ContentStyle="max-width:80%; min-width:50%; max-heigh: 80%; min-heigh: 80%; width:1000px ">
    <DialogContent>
        <MudImage class="object-center" Src=@Image Fluid="true" ObjectFit="ObjectFit.Fill" Elevation="25" />
        
    </DialogContent>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public string Image { get; set; }
}